{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/common": ["libs/common/src"],
      "@app/common/*": ["libs/common/src/*"],
      "@app/database": ["libs/database/src"],
      "@app/database/*": ["libs/database/src/*"],
      "@app/redis": ["libs/redis/src"],
      "@app/redis/*": ["libs/redis/src/*"],
      "@app/kafka": ["libs/kafka/src"],
      "@app/kafka/*": ["libs/kafka/src/*"],
      "@app/grpc": ["libs/grpc/src"],
      "@app/grpc/*": ["libs/grpc/src/*"],
      "@app/auth-db": ["libs/auth-db/src"],
      "@app/auth-db/*": ["libs/auth-db/src/*"],
      "@app/video-db": ["libs/video-db/src"],
      "@app/video-db/*": ["libs/video-db/src/*"],
      "@app/interaction-db": ["libs/interaction-db/src"],
      "@app/interaction-db/*": ["libs/interaction-db/src/*"],
      "@app/notification-db": ["libs/notification-db/src"],
      "@app/notification-db/*": ["libs/notification-db/src/*"]
    }
  }
}
